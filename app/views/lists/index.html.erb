<div class="banner" text-color="white">
  <h2>My movies</h2>
</div>
<div class="container">
  <h1>My lists</h1>
</div>
<div class="container">
  <% @lists.each do |list| %>
    <%= link_to list_path(list) do %>
      <p><%= list.name %></p>
     <% end %>
  <% end %>
  <%= link_to "New list", new_list_path, class: "btn btn-success mr-2"%>
</div>
