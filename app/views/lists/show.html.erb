<div class="container">
  <h1>Movies</h1>
  <%= link_to "Add a movie", new_list_bookmark_path(@list), class: "btn btn-success mr-2" %>
</div>

<div class="container">
  <% @list.movies.each do |movie| %>
    <%= movie.title %>
    <%= movie.overview %>
    <%= movie.poster_url %>
    <%= movie.rating %>
  <% end %>
</div>
